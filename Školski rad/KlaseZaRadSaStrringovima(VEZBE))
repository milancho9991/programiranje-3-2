        string link = "https://ets.edu.rs/";
        Console.WriteLine(link);
        string[] splitovanLink = link.Split('/');
        Console.WriteLine(splitovanLink[0]);
        Console.WriteLine(splitovanLink[1]);
        Console.WriteLine(splitovanLink[2]);
        string novoSplitovanje = splitovanLink[2];
        string[] splitovanje2 = novoSplitovanje.Split(".");
        Console.WriteLine(splitovanje2[0]);
        Console.WriteLine(splitovanje2[1]);
        string imeSkole = splitovanje2[0];
        imeSkole = imeSkole.ToUpper();
        Console.WriteLine(imeSkole);
        imeSkole = imeSkole.Replace("E", "Elektro");
        Console.WriteLine(imeSkole);
        imeSkole = imeSkole.Replace("T", "tehnička");
        Console.WriteLine(imeSkole);
        imeSkole = imeSkole.Replace("S", " Škola");
        Console.WriteLine(imeSkole);

    }
}
